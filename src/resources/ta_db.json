{
  "MA": {
    "name": "Moving Average",
    "endpoint": "https://api.taapi.io/ma?secret={api_key}&exchange=binance",
    "ref": "https://taapi.io/indicators/moving-average/",
    "params": [
      [
        "interval",
        "Time frame for candles: 1m, 5m, 15m, 30, 1h, 2h, 4h, 12h, 1d, 1w",
        true
      ],
      [
        "period",
        "Length / period of the moving average. It sets the number of candles used in the MA calculation (Default = 30)",
        false
      ]
    ],
    "output": [
      "value"
    ]
  },
  "EMA": {
    "name": "Exponential Moving Average",
    "endpoint": "https://api.taapi.io/ema?secret={api_key}&exchange=binance",
    "ref": "https://taapi.io/indicators/exponential-moving-average/",
    "params": [
      [
        "interval",
        "Time frame for candles: 1m, 5m, 15m, 30, 1h, 2h, 4h, 12h, 1d, 1w",
        true
      ],
      [
        "period",
        "Length / period of the EMA. It sets the number of candles used in the EMA calculation (Default = 30)",
        false
      ]
    ],
    "output": [
      "value"
    ]
  },
  "SMA": {
    "name": "Simple Moving Average",
    "endpoint": "https://api.taapi.io/sma?secret={api_key}&exchange=binance",
    "ref": "https://taapi.io/indicators/simple-moving-average/",
    "params": [
      [
        "interval",
        "Time frame for candles: 1m, 5m, 15m, 30, 1h, 2h, 4h, 12h, 1d, 1w",
        true
      ],
      [
        "period",
        "Length / period of the SMA. It sets the number of candles used in the SMA calculation (Default = 30)",
        false
      ]
    ],
    "output": [
      "value"
    ]
  },
  "RSI": {
    "name": "Relative Strength Index",
    "endpoint": "https://api.taapi.io/rsi?secret={api_key}&exchange=binance",
    "ref": "https://taapi.io/indicators/relative-strength-index-rsi/",
    "params": [
      [
        "interval",
        "Time frame for candles: 1m, 5m, 15m, 30, 1h, 2h, 4h, 12h, 1d, 1w",
        true
      ],
      [
        "period",
        "Adjust the RSI length- number of candles used to calculate RSI (Default = 14)",
        false
      ]
    ],
    "output": [
      "value"
    ]
  },
  "BBANDS": {
    "name": "Bollinger Bands",
    "endpoint": "https://api.taapi.io/bbands2?secret={api_key}&exchange=binance",
    "ref": "https://taapi.io/indicators/bollinger-bands-another-implementation/",
    "params": [
      [
        "interval",
        "Time frame for candles: 1m, 5m, 15m, 30, 1h, 2h, 4h, 12h, 1d, 1w",
        true
      ],
      [
        "period",
        "The period length to consider in the data set (Default = 20)",
        false
      ],
      [
        "stddev",
        "Standard deviation value of the price (Default = 2)",
        false
      ]
    ],
    "output": [
        "valueUpperBand",
        "valueMiddleBand",
        "valueLowerBand"
    ]
  }
}